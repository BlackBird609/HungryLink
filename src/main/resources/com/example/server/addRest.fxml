<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="625.0" prefWidth="875.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.server.AddRestController">
   <children>
      <ImageView fitHeight="625.0" fitWidth="875.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="36.0" layoutY="44.0" mnemonicParsing="false" onAction="#OnACtionBackButton" text="Back" />
      <AnchorPane layoutX="231.0" layoutY="79.0" prefHeight="427.0" prefWidth="399.0">
         <children>
            <VBox layoutX="34.0" prefHeight="386.0" prefWidth="350.0" spacing="5.0">
               <children>
                  <Label text="Restaurant Id" />
                  <TextField fx:id="id" onAction="#OnACtionID" onMouseClicked="#OnACtionID" prefHeight="30.0" prefWidth="350.0" />
                  <Label lineSpacing="5.0" prefHeight="38.0" prefWidth="145.0" text="Restaurant Name" />
                  <TextField fx:id="name" onAction="#OnACtionID" onMouseClicked="#OnACtionID" />
                  <Label text="Score" />
                  <TextField fx:id="score" onAction="#OnACtionID" onMouseClicked="#OnACtionID" />
                  <Label text="Price" />
                  <TextField fx:id="price" onAction="#OnACtionID" onMouseClicked="#OnACtionID" />
                  <Label text="Zip Code" />
                  <TextField fx:id="zipCode" onAction="#OnACtionID" onMouseClicked="#OnACtionID" />
                  <Label text="Category 1" />
                  <TextField fx:id="category1" onAction="#OnACtionID" onMouseClicked="#OnACtionID" />
                  <Label text="Category 2 (optional)" />
                  <TextField fx:id="category2" onAction="#OnACtionID" onMouseClicked="#OnACtionID" />
                  <Label text="Category 3 (optional)" />
                  <TextField fx:id="category3" onAction="#OnACtionID" onMouseClicked="#OnACtionID" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
      <Button layoutX="263.0" layoutY="515.0" mnemonicParsing="false" onAction="#OnACtionCancelButton" text="Cancel" />
      <Button layoutX="566.0" layoutY="515.0" mnemonicParsing="false" onAction="#OnACtionSubmitButton" text="Submit" />
      <AnchorPane fx:id="statusShowingAnchor" layoutX="198.0" layoutY="43.0" prefHeight="26.0" prefWidth="512.0" />
   </children>
</AnchorPane>
