<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="625.0" prefWidth="875.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.server.HomePageController">
   <children>
      <ImageView fitHeight="627.0" fitWidth="877.0" pickOnBounds="true">
         <image>
            <Image url="@pictures/home.jpeg" />
         </image>
      </ImageView>
      <Button fx:id="buttonStartRestaurantServer" layoutX="264.0" layoutY="163.0" mnemonicParsing="false" onAction="#OnActionRestaurantButton" text="Start Restaurant Server" />
      <Button fx:id="buttonStartCustomerServer" layoutX="559.0" layoutY="163.0" mnemonicParsing="false" onAction="#OnActionStartCustomerButton" text="Start Customer Server" />
      <Button fx:id="buttonExit" layoutX="812.0" layoutY="14.0" mnemonicParsing="false" onAction="#OnActionExitButton" text="Exit" />
      <Button fx:id="buttonAddRest" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#OnActionAddRest" text="Add Restaurant" />
      <Button fx:id="showStatusButtonId" layoutX="14.0" layoutY="270.0" mnemonicParsing="false" onAction="#OnActionShowStatus" text="Show Status" />
      <AnchorPane fx:id="statusShowingAnchorPane" layoutY="307.0" prefHeight="321.0" prefWidth="875.0">
         <children>
            <TextArea fx:id="showText" prefHeight="321.0" prefWidth="875.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
